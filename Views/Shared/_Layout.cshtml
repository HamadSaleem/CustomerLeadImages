
@{
    Layout = null;
    var title = ViewData["Title"] as string ?? "Customer/Lead Images";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@title</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/site.css" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css" integrity="sha512-SnS4ZP3lH8eo8u+P8M6VvV4JvC9n0Lk6wF1H3lZbU8sFqg9mU7+7j4D4Yw2b1s2m9C5cO3qCk1vQY0qvB3Z8Ww==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <div class="container">
    <a class="navbar-brand" href="/">Images</a>
    <div class="collapse navbar-collapse">
      <ul class="navbar-nav me-auto mb-2 mb-lg-0">
        <li class="nav-item"><a class="nav-link" href="/Customers">Customers</a></li>
        <li class="nav-item"><a class="nav-link" href="/Leads">Leads</a></li>
      </ul>
    </div>
  </div>
</nav>
<div class="container my-4">
    @RenderBody()
</div>
<script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/js/site.js"></script>
@RenderSection("Scripts", required: false)
</body>
</html>
